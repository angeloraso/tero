{
  "name": "tero",
  "version": "1.5.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "start:pwa": "npm run build && http-server -o -p 4200 -c-1 dist",
    "pwa:add-assets": "node src/scripts/pwa-assets.ts",
    "pwa:rm-assets": "node src/scripts/pwa-assets.ts",
    "build": "ng build --configuration=production",
    "build:mobile": "ng build --configuration=mobile",
    "android:resources": "capacitor-assets generate --android",
    "android:config": "VERSION_NAME=$npm_package_version npx trapeze run config.yaml --android -y",
    "android": "rm -rf ./android && npm run build:mobile && capacitor add android && capacitor sync android && npm run android:resources && npm run android:config && capacitor open android"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.6",
    "@angular/cdk": "16.2.5",
    "@angular/common": "16.2.6",
    "@angular/compiler": "16.2.6",
    "@angular/core": "16.2.6",
    "@angular/forms": "16.2.6",
    "@angular/material": "16.2.5",
    "@angular/platform-browser": "16.2.6",
    "@angular/platform-browser-dynamic": "16.2.6",
    "@angular/router": "16.2.6",
    "@angular/service-worker": "16.2.6",
    "@awesome-cordova-plugins/call-number": "6.4.0",
    "@awesome-cordova-plugins/core": "6.4.0",
    "@capacitor/android": "5.3.0",
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.3.0",
    "@capacitor/splash-screen": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@capacitor/toast": "5.0.6",
    "@ngx-translate/core": "15.0.0",
    "call-number": "1.0.1",
    "chartist": "1.3.0",
    "firebase": "10.4.0",
    "fuse.js": "6.6.2",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "uuid4": "2.0.3",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.3",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "16.2.3",
    "@angular/compiler-cli": "16.2.6",
    "@capacitor/assets": "2.0.4",
    "@capacitor/cli": "5.3.0",
    "@trapezedev/configure": "7.0.10",
    "@types/node": "20.7.0",
    "@types/uuid4": "2.0.1",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-angular": "4.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "http-server": "14.1.1",
    "prettier": "3.0.3",
    "prettier-eslint": "15.0.1",
    "typescript": "5.0.4"
  }
