{
  "name": "tero",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "start:pwa": "npm run build && npx http-server -o -p 4200 -c-1 dist",
    "pwa:add-assets": "node src/scripts/pwa-assets.ts",
